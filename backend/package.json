{
  "name": "clube-da-setima-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run prisma:migrate:run && node dist/src/server.js",
    "prisma:seed": "npx prisma db seed",
    "prisma:migrate:generate": "npx prisma migrate dev",
    "prisma:migrate:run": "prisma migrate deploy",
    "dev": "npx dotenv -e .env.development nodemon src/server.ts",
    "dev:prisma:migrate:run": "npx dotenv -e .env.development prisma migrate deploy",
    "dev:prisma:migrate:generate": "npx dotenv -e .env.development prisma migrate dev",
    "dev:prisma:seed": "npx dotenv -e .env.development prisma db seed",
    "test": "npx dotenv -e .env.test npx jest",
    "test:prisma:migrate:run": "npx dotenv -e .env.test prisma migrate deploy",
    "test:prisma:migrate:generate": "npx dotenv -e .env.test prisma migrate dev",
    "teste:prisma:seed": "npx dotenv -e .env.test prisma db seed"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Clube-da-Setima/clube-da-setima-backend.git"
  },
  "keywords": [
    "back-end",
    "typescript",
    "postgres",
    "prisma",
    "express",
    "nodejs"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Clube-da-Setima/clube-da-setima-backend/issues"
  },
  "homepage": "https://github.com/Clube-da-Setima/clube-da-setima-backend#readme",
  "dependencies": {
    "@brazilian-utils/brazilian-utils": "^1.0.0-rc.12",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.1.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.2.0",
    "joi": "^17.9.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@prisma/client": "^4.15.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "dotenv-cli": "^7.2.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prisma": "^4.15.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
